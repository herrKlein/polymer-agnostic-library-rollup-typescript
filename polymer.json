{
  "entrypoint": "index.html",
  "shell": "build/index.js",
  "fragments": [
    "build/lazy-element.js"
  ],
  "sources": [
   "build/index.js",
   "package.json"
  ],
  "extraDependencies": [
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [
